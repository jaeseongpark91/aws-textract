    "def splitFiles_forUpload(dataframe, fileLength=200):\n",
    "    n_rows = dataframe.shape[0]\n",
    "    n_outputfiles = int(n_rows/fileLength)+1\n",
    "    print(\"Exporting \" + str(n_outputfiles) + \" files...\")\n",
    "    for i in range(1, n_outputfiles+1):\n",
    "        output = dataframe.iloc[(i-1)*fileLength:i*fileLength]\n",
    "        output.to_csv('output_{}.csv'.format(i), index=False)\n",
    "\n",
    "        "
   ]
