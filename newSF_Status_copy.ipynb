    "current_pathway_query = (\n",
    "    '''\n",
    "    # query to find pathways with current pathways. This is different than having \"completed pathway status\"\n",
    "select currentTable.*, Total_Accounts, case when count_current = Total_Accounts then 'current' else 'not_current' end as 'current_status'\n",
