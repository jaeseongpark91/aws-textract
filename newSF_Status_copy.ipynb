    "def createDataLoaderFormat(dataFrame, status, issueType = \"Use_Default_message\"):##This function generates a dataframe with a format capaable of upload into SF\n",
    "    ##the data frame passed into this functions needs to have propertyId but otherwise can have other \n",
    "    #idea for this process is to insert the relevant status \n",
    "    ### creating another shell df this one in the format that can be uploaded into dataLoader IO\n",
    "    dfDataLoaderFormat = pd.DataFrame(columns=['sf_property_id', 'Owner_Pathway_Status_Electric__c','Owner_Pathway_Status_Gas__c', 'Owner_Pathway_Status_Water__c', 'Owner_Pathway_Notes_Electric__c', 'Owner_Pathway_Notes_Gas__c', 'Owner_Pathway_Notes_Water__c'])\n",
    "    dfDataLoaderFormat['sf_property_id'] = dataFrame['sf_property_id'].unique() ##generates a list of the properties that we have updated statuses for\n",
    "    for r in range(0,len(dataFrame)):\n",
    "\n",
    "            pID_col_pos = dataFrame.columns.get_loc(\"sf_property_id\")\n",
    "            propertyId = dataFrame.iloc[r,pID_col_pos] ##pulls the property id from the relevent row\n",
    "            rowInsert = dfDataLoaderFormat.index[dfDataLoaderFormat ['sf_property_id'] == propertyId ] ##finds the correct row in the dataLoader df to insert this row into\n",
    "\n",
    "            uti_col_pos = dataFrame.columns.get_loc(\"esc_utility_type\")\n",
    "            utilityType=dataFrame.iloc[r,uti_col_pos]\n",
    "\n",
    "            message = '' ##could convert to ifelse clause\n",
    "            if (issueType == \"pay_for_data_no\"):\n",
    "                message = \"There are accounts at this pathway with pay for data marked as no. Please review\"\n",
    "            if (issueType == 'broken_accounts'): ## need to figure out a way to automate messages but for now this will do \n",
    "                message = \"There are \" + str(dataFrame.iloc[r,dataFrame.columns.get_loc(\"Broken_Accounts\")]) + \" broken accounts out of \" + str(dataFrame.iloc[r,dataFrame.columns.get_loc(\"Total_Accounts\") ]) + \" accounts at this pathway with an average bill cycles behind of: \" + str(dataFrame.loc[r,'avg_Bill_Cycles_Behind'])\n",
    "            if (issueType == 'critical_account_setup_errors_Query'):\n",
    "                message = \"There are \" + str(dataFrame.iloc[r,dataFrame.columns.get_loc(\"Broken_Accounts\") ]) + \" account(s) \" + str(dataFrame.iloc[r,dataFrame.columns.get_loc('Issue')]) + \" this is a crticial account setup issue and we cannot activate these account(s) with urjanet until this is resolved \"        \n",
    "            if (issueType == 'broken_logins'): ## need to figure out a way to automate messages but for now this will do \n",
    "                message = \"There are \" + str(dataFrame.iloc[r,dataFrame.columns.get_loc(\"Broken_Accounts\")]) + \"  accounts with broken logins flagged out of \" + str(dataFrame.iloc[r,dataFrame.columns.get_loc(\"Total_Accounts\")]) + \". New data will not arrive for these accounts until the correct logins are updated in ESC.\" \n",
    "\n",
    "            if (utilityType == 'E'):  ##insert into electric status column \n",
    "                dfDataLoaderFormat.loc[rowInsert,'Owner_Pathway_Status_Electric__c'] = status\n",
    "                dfDataLoaderFormat.loc[rowInsert,'Owner_Pathway_Notes_Electric__c'] = message            \n",
    "            if (utilityType == 'G'):  ##insert into gas status column \n",
    "                dfDataLoaderFormat.loc[rowInsert,'Owner_Pathway_Status_Gas__c'] = status\n",
    "                dfDataLoaderFormat.loc[rowInsert,'Owner_Pathway_Notes_Gas__c'] = message \n",
    "            if (utilityType == 'W'): ##insert into water status column \n",
    "                dfDataLoaderFormat.loc[rowInsert,'Owner_Pathway_Status_Water__c'] = status\n",
    "                dfDataLoaderFormat.loc[rowInsert,'Owner_Pathway_Notes_Water__c'] = message \n",
    "    return dfDataLoaderFormat\n",
    "       "
   ]
