    "def reformatPropertyPathway(): ##This method generates a table with a format that can be joined to utility pathway level information\n",
    "    propertyStatus_query = (\n",
    "    '''   \n",
    "     select   pc.id,  pc.site_id__c,pc.Owner_Pathway_Status_Electric__c as 'E', Owner_Pathway_Status_Gas__c as 'G', pc.Owner_Pathway_Status_Water__c as 'W', pc.Owner_Pathway_Notes_Electric__c, pc.Owner_Pathway_Notes_Gas__c, pc.Owner_Pathway_Notes_Water__c\n",
    "        FROM salesforce_replica.Property__c pc\n",
    "        where  (pc.Account__c = '0015000000jKRs6AAG' or  pc.Portfolio__c = 'a3i380000005FedAAE')\n",
    "    '''\n",
    "    )\n",
    "    ##propertyStatusDF This table contains info from Salesforce about the status of pathways\n",
    "    #notes fields will be added in a future update\n",
    "    ##ideally we could connect directly to the databased rather than reading in a CSV. This will happen in a later update. \n",
    "    propertyStatusDF = pd.read_sql_query(propertyStatus_query, cnt)\n",
    "    ##creating shell dataframe (this will be the returned dataframe at the end of the function)\n",
    "    dfJoinShell = pd.DataFrame(columns=['site_id', 'esc_utility_type','sf_property_id', 'Status','Notes'])\n",
    "    ##this table will be used to join to the utility pathway proxy output from esc_utility_account_monitoring table\n",
    "    ##nested for loop will read through every item in the propertyStatusDF\n",
    "    for r in range(0,len(propertyStatusDF)): ##this first for loop is going through each row of table(yes i know there are other ways to do this)\n",
    "        for c in range(2,5): ##This loop is cycling though the 3 columns that contain statuses (this can be expanded to include the notes fields in a later iteration)\n",
    "            if c == 2: ##these conditions rely on the status columns being ordered electric gas water. if they are in a different order these conditionals would need to be adjusted\n",
    "                dfJoinShell.copy().loc[len(dfJoinShell.index)] = [propertyStatusDF.iloc[r,1], \"E\", propertyStatusDF.iloc[r,0], propertyStatusDF.iloc[r,c], propertyStatusDF.iloc[r,c+3]]## see this logic broken out below\n",
    "            if c == 3:\n",
    "                dfJoinShell.copy().loc[len(dfJoinShell.index)] = [propertyStatusDF.iloc[r,1], \"G\", propertyStatusDF.iloc[r,0], propertyStatusDF.iloc[r,c],propertyStatusDF.iloc[r,c+3]]\n",
    "            if c == 4:\n",
    "                dfJoinShell.copy().loc[len(dfJoinShell.index)] = [propertyStatusDF.iloc[r,1], \"W\",propertyStatusDF.iloc[r,0], propertyStatusDF.iloc[r,c],propertyStatusDF.iloc[r,c+3]]\n",
    "    return dfJoinShell"
   ]
  },
