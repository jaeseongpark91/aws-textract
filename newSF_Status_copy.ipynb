    "\t\t\t\t\tand drs_source = 'utility_website' and drs_method like 'urj%'\n",
    "\t\t\t\t\tand who_pays = 'O'\n",
    "\t\t\t\t\tand date_closed is null\n",
    "\t\t\t\t\tand drs_pay_for_data = 'Yes'\n",
    "\t\t\t\t\t#and in_orient = 1  \n",
    "\t\t\t\t\tand esc_account_name not like '%baseline%'\n",
    "\t\t\t\t\tand esc_account_name not like '%estimate%'\n",
    "\t\t\t\t\tand esc_account_name not like '%aggregate%'\n",
    "\t\t\t\t\tand DATEDIFF(CURRENT_DATE(),esc_most_recent_bill_end_date)/urj_start_to_end_avg < 3 ## should be billing cycle\n",
    "                    and (sf_account ='Fannie Mae' or site_id in ({}))\n",
