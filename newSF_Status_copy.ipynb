    "critical_account_setup_errors_query = (\n",
    "    '''\n",
    "    select site_id, esc_utility_type,   \n",
    "\tcase \n",
    "\t\twhen esc_account_num_clean is null and esc_provider_name like 'Unknown%' then 'Missing Account Number & Unknown Provider in ESC'\t\n",
    "\t\twhen has_login = 0 then 'Missing Login/password in ESC'\n",
    "\t\twhen esc_account_num_clean is null  then 'Missing Account Number in ESC'\n",
    "\t\twhen esc_provider_name like 'Unknown%' then 'Unknown Provider in ESC' \n",
    "\t\twhen drs_retrieval_type = 'None' then 'Retrival type is set to none'\n",
    "\tend as 'Issue', count(*) as Broken_Accounts\n",
    "\tfrom bp_corraled_datasets.esc_utility_account_monitoring euam \n",
    "\twhere  \n",
    "\tsf_account = 'Fannie Mae' \n",
    "\tand who_pays = 'O'\n",
    "\tand date_closed is null\n",
    "\tand drs_method like 'Urjanet'\n",
    "\tand (has_login = 0 or esc_account_num_clean is null or esc_provider_name like '%Unknown%' or ( drs_pay_for_data = 'Yes'and drs_retrieval_type = 'None'))\n",
    "\tgroup by site_id, esc_utility_type \n",
    "\torder by Issue\n",
    "    '''\n",
    ")"
   ]
