    "def pull_utillity_pathways(query): ##this function will pull ESC accounts of a given issue and then join that table to the existing SF statuses \n",
    "    if (query == \"out_of_date_urj_query\"):\n",
    "        upDf  = pd.read_sql_query(out_of_date_urj_query, cnt)\n",
    "    elif (query == \"critical_account_setup_errors_query\"):\n",
    "        upDf  = pd.read_sql_query(critical_account_setup_errors_query, cnt)\n",
    "    elif (query == \"urj_flagged_broken_logins_query\"):\n",
    "        upDf  = pd.read_sql_query(urj_flagged_broken_logins_query, cnt)\n",
    "    elif (query == \"pay_for_data_no_query\"):\n",
    "        upDf  = pd.read_sql_query(pay_for_data_no_query, cnt)\n",
    "    elif (query == \"current_pathway_query\"):\n",
    "        upDf  = pd.read_sql_query(current_pathway_query, cnt)\n",
    "\n",
    "    dfJoinShell = reformatPropertyPathway()\n",
    "    dfJoinShell.loc[\"site_id\"] = pd.to_numeric(dfJoinShell.loc[:,\"site_id\"])\n",
    "    upDf.loc[:,\"site_id\"] = pd.to_numeric(upDf.loc[:,\"site_id\"])\n",
    "    ##joining ESC data with SF data\n",
    "    mergedDF= pd.merge(upDf,dfJoinShell,how = 'left', on = ['site_id','esc_utility_type']) ## could be expanded to include multiple types of joins. \n",
    "    return mergedDF"
   ]
  },
