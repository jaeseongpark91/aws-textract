   "source": [
    "%%time\n",
    "##Right now I have the query setup for flagging pathways with out of date data from Urjanet but we can swap any query that is going to flag issues here. \n",
    "out_of_date_urj_query = (\n",
    "    '''   \n",
    "    select errorTable.*, totalTable.Total_Accounts\n",
    "FROM \n",
    "(\n",
    "select site_id, esc_utility_type , esc_most_recent_bill_end_date, orient_most_recent_bill_end_date, ROUND(AVG(DATEDIFF(CURRENT_DATE(),orient_most_recent_bill_end_date)/urj_start_to_end_avg)) as 'avg_Bill_Cycles_Behind', urj_monitoring_status, urj_message, count(DISTINCT bpAccountUuid) as 'Broken_Accounts'\n",
    "from bp_corraled_datasets.esc_utility_account_monitoring \n",
    "where #urj_next_expected_post_date is not null \n",
    " drs_source = 'utility_website' and drs_method like 'urj%'\n",
    "                   \tand who_pays = 'O'\n",
    "\t\t\t\t\tand date_closed is null\n",
    "\t\t\t\t\tand drs_pay_for_data = 'Yes'\n",
    "\t\t\t\t\tand in_orient = 1  \n",
    "\t\t\t\t\tand DATEDIFF(CURRENT_DATE(),orient_most_recent_bill_end_date)/urj_start_to_end_avg > 4 ## should be billing cycle \n",
    "                    and (sf_account ='Fannie Mae' or site_id in ({}))\n",
    "\t\t\t\t\t#and urj_monitoring_status = 'PRODUCTION'\n",
    "                    group by site_id, esc_utility_type \n",
    "                    order by 1\n",
    ") errorTable\n",
    "left JOIN \n",
    "(\n",
    "select site_id, esc_utility_type, count(DISTINCT bpAccountUuid) as 'Total_Accounts'\n",
    "from bp_corraled_datasets.esc_utility_account_monitoring \n",
    "where #urj_next_expected_post_date is not null \n",
    " drs_source = 'utility_website' and drs_method like 'urj%'\n",
    "                   \tand who_pays = 'O'\n",
    "\t\t\t\t\tand date_closed is null\n",
    "\t\t\t\t\tand drs_pay_for_data = 'Yes'\n",
    "                    and (sf_account ='Fannie Mae' or site_id in ({}))\n",
    "\t\t\t\t\t#and urj_monitoring_status = 'PRODUCTION'\n",
    "                    group by site_id, esc_utility_type \n",
    "                    order by 1\n",
    ") totalTable \n",
    "on errorTable.site_id = totalTable.site_id and errorTable.esc_utility_type = totalTable.esc_utility_type   \n",
    "    '''.format(fnma_sites, fnma_sites)\n",
    "    )"
   ]
