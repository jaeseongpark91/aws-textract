    "existing_pathways_query = (\n",
    "    '''\n",
    "        ##Provides list of pathways for active FNMA Compliance Submissions for a given benchmarking year\n",
    "        select up.Id, p.site_id__c as site_id, CASE\n",
    "            WHEN up.Fuel_Type__c = 'Electric' THEN 'E'\n",
    "            WHEN up.Fuel_Type__c = 'Gas' THEN 'G'\n",
    "            WHEN up.Fuel_Type__c = 'Water/Sewer' THEN 'W'\n",
    "            ELSE up.Fuel_Type__c\n",
    "        END esc_utility_type\n",
    "        FROM salesforce_replica.Compliance_Submissions__c cs\n",
    "        LEFT JOIN salesforce_replica.Property__c p ON cs.Property__c = p.Id\n",
    "        right join salesforce_replica.Utility_Pathway__c up ON up.Compliance_Submission__c = cs.Id\n",
    "        where \n",
    "        ##cs.Benchmarking_Year__c  = 2020\n",
    "        (cs.Benchmarking_Year__c  = 2020 or cs.Benchmarking_Year__c  = 2021) \n",
    "        and cs.RecordTypeId = '0121T00000023x2QAA'\n",
    "        and up.Fuel_Type__c in  ('Electric','Gas','Water/Sewer')\n",
    "    '''\n",
    ")"
   ]
  },
